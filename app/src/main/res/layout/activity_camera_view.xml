<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    tools:context=".henCoder.CustomViewActivity">

    <com.varmin.vdemo.view.FlipView
        android:id="@+id/view_flip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <com.varmin.vdemo.view.MaterialEditText
        android:id="@+id/met_edit"
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="say 666"
        android:background="@color/base_app_alert_bg"/>
    <com.varmin.vdemo.view.LabelTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:includeFontPadding="false"
        android:background="@color/base_app_alert_bg"
        android:textSize="30sp"
        android:text="TAG 206974 我的 ggjjffpp"/>
    <com.varmin.vdemo.view.LabelView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TAG 206974"
        android:textSize="30sp"
        android:textColor="@color/base_text_black_666666"
        />
    <com.varmin.vdemo.view.LabelView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TAG 206974 我的 ggjjffpp"
        android:textSize="30sp"
        android:textColor="@color/base_text_black_666666"
        app:round_radius="5dp"
        app:is_bound="false"
        />
    <com.varmin.vdemo.view.LabelLayout
        android:id="@+id/ll_la"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</LinearLayout>